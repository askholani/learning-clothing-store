// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model products {
  id                Int       @id(map: "Products_pkey")
  name              String?
  description       String?
  image             String?
  stock             Int?
  price             Int?
  id_category       Int?
  id_lookbook       Int?
  id_new_collection Int?
  id_discount       Int?
  id_rating         Int?
  id_review         Int?
  created_at        DateTime  @default(now())
  updated_at        DateTime?
  deleted_at        DateTime?
} 
